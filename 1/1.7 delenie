import java.util.Scanner;
import java.util.Locale;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int a = sc.nextInt();
        int b = sc.nextInt();
        int i=0;
        int n=0;

       do {
           if (b>a) {n = 0; i = a; break;}
        i= a-b;
        a = a-b;
        n++;
       } while (a>=b);

        System.out.println(n + " "+ i);
    }
}

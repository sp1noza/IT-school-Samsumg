import java.util.Scanner;
import java.util.Locale;

public class Main {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        int j = 0;
        int[] a;
        a = new int[n];

        for (int i = 0; i < n; i++) {
            a[i] = sc.nextInt();

        }
        int x = sc.nextInt();
        for (int i = 0; i < n; i++) {
            if (a[i] == x){
                j=i+1; break;
            };

        }
        if(j>0){
        System.out.println(j);}
        else {System.out.println("NO");}
    }
}



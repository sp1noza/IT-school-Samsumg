import java.util.Scanner;
import java.util.Locale;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        in.useLocale(Locale.US);
        double a;
        a = in.nextDouble();
        if (a >= 3 && a <= 8) {
            System.out.println("true");
        } else {
            System.out.println("false");
        }
    }
}
